<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.home_ui.DiveLogActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_8"
            android:layout_marginBottom="@dimen/dp_11"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/dive_log_back"
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:background="@color/c_000000"
                android:src="@mipmap/back"
                tools:ignore="ContentDescription" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="@string/add_log"
                android:textColor="@color/c_000000"
                android:textSize="@dimen/sp_18"
                android:textStyle="normal" />
        </RelativeLayout>
        <android.support.design.widget.TabLayout
            android:id="@+id/dive_log_tab"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_35"
            android:layout_marginLeft="@dimen/dp_11"
            android:layout_marginRight="@dimen/dp_11"
            app:tabIndicatorHeight="@dimen/dp_0"
            app:tabTextColor="@color/c_000000"
            app:tabSelectedTextColor="@color/c_ffffff"
            app:tabBackground="@drawable/dive_log_selector_tab"
            />
        <!--<include-->
            <!--layout="@layout/layout_divelog_tab"-->
            <!--&gt;-->
        <!--</include>-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/dp_16"
            >
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:overScrollMode="never"
                android:scrollbarSize="@dimen/dp_0">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/dp_11"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/sp_16"
                        android:text="@string/dive_data"
                        android:textColor="@color/blue"
                        android:textSize="@dimen/sp_14" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/sp_16"
                        android:text="@string/dive_data_cishu"
                        android:textColor="@color/c_000000"
                        android:textSize="@dimen/sp_12" />

                    <EditText
                        android:id="@+id/dive_log_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_11"
                        android:layout_marginRight="@dimen/dp_31"
                        android:background="@drawable/shape_et"
                        android:gravity="center"
                        android:hint="2"
                        android:inputType="phone|number"
                        android:lines="1"
                        android:maxLines="1"
                        android:paddingTop="@dimen/dp_3"
                        android:paddingBottom="@dimen/dp_4"
                        android:textSize="@dimen/sp_14"
                        android:textStyle="normal" />

                    <!--最大深度-->
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/sp_13"
                            android:text="@string/max_depth"
                            android:textColor="@color/c_000000"
                            android:textSize="@dimen/sp_12" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_144"
                            android:layout_marginTop="@dimen/sp_13"
                            android:text="@string/min_temperature"
                            android:textColor="@color/c_000000"
                            android:textSize="@dimen/sp_12" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/sp_13">
                        <!--最大深度-->
                        <RelativeLayout
                            android:layout_width="@dimen/dp_138"
                            android:layout_height="wrap_content"
                            android:background="@drawable/shape_et"
                            android:orientation="horizontal"
                            android:paddingTop="@dimen/dp_3"
                            android:paddingBottom="@dimen/dp_4">

                            <EditText
                                android:id="@+id/max_depth"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:background="@null"
                                android:gravity="center"
                                android:hint="2"
                                android:inputType="phone|number"
                                android:lines="1"
                                android:maxLines="1"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="@dimen/dp_10"
                                android:text="m/ft"
                                android:textColor="@color/c_000000"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal" />
                        </RelativeLayout>
                        <!--最低溫度-->
                        <RelativeLayout
                            android:layout_width="@dimen/dp_138"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_144"
                            android:background="@drawable/shape_et"
                            android:orientation="horizontal"
                            android:paddingTop="@dimen/dp_3"
                            android:paddingBottom="@dimen/dp_4">

                            <EditText
                                android:id="@+id/min_temperature"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:background="@null"
                                android:gravity="center"
                                android:hint="2"
                                android:inputType="phone|number"
                                android:lines="1"
                                android:maxLines="1"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="@dimen/dp_10"
                                android:text="C/F"
                                android:textColor="@color/c_000000"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal" />
                        </RelativeLayout>

                    </RelativeLayout>
                    <!--===============平均深度/温度========================================-->
                    <RelativeLayout
                        android:id="@+id/mean_rl"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/sp_13"
                            android:text="@string/mean_depth"
                            android:textColor="@color/c_000000"
                            android:textSize="@dimen/sp_12" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_144"
                            android:layout_marginTop="@dimen/sp_13"
                            android:text="@string/mean_temperature"
                            android:textColor="@color/c_000000"
                            android:textSize="@dimen/sp_12" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/mean_rl_et"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/sp_13">
                        <!--平均深度-->
                        <RelativeLayout
                            android:layout_width="@dimen/dp_138"
                            android:layout_height="wrap_content"
                            android:background="@drawable/shape_et"
                            android:orientation="horizontal"
                            android:paddingTop="@dimen/dp_3"
                            android:paddingBottom="@dimen/dp_4">

                            <EditText
                                android:id="@+id/mean_depth_et"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:background="@null"
                                android:gravity="center"
                                android:hint="2"
                                android:inputType="phone|number"
                                android:lines="1"
                                android:maxLines="1"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="@dimen/dp_10"
                                android:text="m/ft"
                                android:textColor="@color/c_000000"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal" />
                        </RelativeLayout>
                        <!--平均溫度-->
                        <RelativeLayout
                            android:layout_width="@dimen/dp_138"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_144"
                            android:background="@drawable/shape_et"
                            android:orientation="horizontal"
                            android:paddingTop="@dimen/dp_3"
                            android:paddingBottom="@dimen/dp_4">

                            <EditText
                                android:id="@+id/mean_temperature"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:background="@null"
                                android:gravity="center"
                                android:hint="2"
                                android:inputType="phone|number"
                                android:lines="1"
                                android:maxLines="1"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="@dimen/dp_10"
                                android:text="C/F"
                                android:textColor="@color/c_000000"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal" />
                        </RelativeLayout>

                    </RelativeLayout>
                    <!--=======================================================-->
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/sp_13"
                            android:text="@string/dive_time"
                            android:textColor="@color/c_000000"
                            android:textSize="@dimen/sp_12" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/sp_13">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_1"
                            android:layout_centerInParent="true"
                            android:layout_marginLeft="@dimen/dp_18"
                            android:layout_marginRight="@dimen/dp_18"
                            android:layout_toLeftOf="@id/time_right"
                            android:layout_toRightOf="@id/time_left"
                            android:background="#BBBBBB" />
                        <!--时间选择-->
                        <RelativeLayout
                            android:id="@+id/time_left"
                            android:layout_width="@dimen/dp_136"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_4"
                            android:background="@drawable/shape_et"
                            android:orientation="horizontal"
                            android:paddingTop="@dimen/dp_3"
                            android:paddingBottom="@dimen/dp_3">
                            <EditText
                                android:id="@+id/selector_time_left"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:background="@null"
                                android:gravity="center"
                                android:hint="@string/selector_time"
                                android:inputType="phone|number"
                                android:lines="1"
                                android:maxLines="1"
                                android:paddingLeft="@dimen/dp_10"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal"/>
                            <ImageView
                                android:id="@+id/selector_time_iv_left"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerInParent="true"
                                android:layout_marginRight="@dimen/dp_10"
                                android:src="@mipmap/black_arrow_down" />
                        </RelativeLayout>

                        <!--时间选择-->
                        <RelativeLayout
                            android:id="@+id/time_right"
                            android:layout_width="@dimen/dp_136"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/dp_37"
                            android:background="@drawable/shape_et"
                            android:orientation="horizontal"
                            android:paddingTop="@dimen/dp_3"
                            android:paddingBottom="@dimen/dp_3">

                            <EditText
                                android:id="@+id/selector_time_right"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:background="@null"
                                android:gravity="center"
                                android:hint="@string/selector_time"
                                android:inputType="phone|number"
                                android:lines="1"
                                android:maxLines="1"
                                android:paddingLeft="@dimen/dp_10"
                                android:textSize="@dimen/sp_14"
                                android:textStyle="normal" />

                            <ImageView
                                android:id="@+id/selector_time_iv_right"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerInParent="true"
                                android:layout_marginRight="@dimen/dp_10"
                                android:src="@mipmap/black_arrow_down" />
                        </RelativeLayout>

                    </RelativeLayout>
                    <!--===========潜水地点========-->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/sp_13"
                        android:text="@string/dive_site"
                        android:textColor="@color/c_000000"
                        android:textSize="@dimen/sp_12" />

                    <EditText
                        android:id="@+id/dive_site_et"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_11"
                        android:layout_marginRight="@dimen/dp_31"
                        android:background="@drawable/shape_et"
                        android:gravity="center"
                        android:inputType="phone|number"
                        android:lines="1"
                        android:maxLines="1"
                        android:paddingTop="@dimen/dp_3"
                        android:paddingBottom="@dimen/dp_4"
                        android:textColor="@color/c_000000"
                        android:textColorHint="@color/c_000000"
                        android:textSize="@dimen/sp_14"
                        android:textStyle="normal" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/dive_idea"
                        android:textColor="@color/blue"
                        android:textSize="@dimen/sp_14" />

                    <EditText
                        android:id="@+id/dive_idea_et"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_107"
                        android:layout_marginTop="@dimen/dp_11"
                        android:layout_marginRight="@dimen/dp_17"
                        android:background="@drawable/shape_et"
                        android:hint="@string/idea_hint"
                        android:textSize="@dimen/sp_14"
                        android:textStyle="normal" />

                    <!--上传照片-->
                    <LinearLayout
                        android:id="@+id/upload_photos"
                        android:layout_width="@dimen/dp_64"
                        android:layout_height="@dimen/dp_64"
                        android:layout_marginTop="@dimen/dp_8"
                        android:background="@drawable/shape_et"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/dp_15"
                            android:layout_height="@dimen/dp_15"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dp_15"
                            android:src="@mipmap/icon_image_add" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/dp_8"
                            android:gravity="center_horizontal"
                            android:text="@string/upload_photos"
                            android:textSize="@dimen/sp_12"
                            android:textStyle="normal" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_6"
                        android:text="@string/tv_attention"
                        android:textSize="@dimen/dp_12" />

                    <!--气瓶消耗-->
                    <TextView
                        android:id="@+id/qiping_xiaohao"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_19"
                        android:text="@string/qiping_xiaohao"
                        android:textColor="@color/blue"
                        android:textSize="@dimen/sp_14"
                        />

                    <LinearLayout
                        android:id="@+id/qiping_xiaohao_ll"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dp_15"
                        android:background="@drawable/shape_et"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_14"
                                android:layout_marginTop="@dimen/dp_9"
                                android:text="气瓶1"
                                android:textColor="@color/c_000000"
                                android:textSize="@dimen/sp_14" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="@dimen/dp_14"
                                android:layout_marginTop="@dimen/dp_9"
                                android:layout_marginRight="@dimen/dp_49"
                                android:text="Bar/Psi"
                                android:textColor="@color/c_000000"
                                android:textSize="@dimen/sp_14" />
                        </RelativeLayout>
                        <!--开始-->
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_14"
                            android:layout_marginTop="@dimen/dp_16"
                            android:text="开始"
                            android:textColor="@color/c_000000"
                            android:textSize="@dimen/sp_12" />

                        <SeekBar
                            android:id="@+id/start_bar"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_13"
                            android:layout_marginTop="@dimen/dp_12"
                            android:layout_marginRight="@dimen/dp_25"
                            android:max="300" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/start_bar_left_tv1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_14"
                                android:layout_marginTop="@dimen/dp_8"
                                android:text="0"
                                android:textColor="@color/c_101010"
                                android:textSize="@dimen/sp_12" />

                            <TextView
                                android:id="@+id/start_bar_right_tv2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginTop="@dimen/dp_8"
                                android:layout_marginRight="@dimen/dp_20"
                                android:text="300"
                                android:textColor="@color/c_101010"
                                android:textSize="@dimen/sp_12" />
                        </RelativeLayout>
                        <!--结束-->
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_14"
                            android:layout_marginTop="@dimen/dp_16"
                            android:text="结束"
                            android:textColor="@color/c_000000"
                            android:textSize="@dimen/sp_12" />

                        <SeekBar
                            android:id="@+id/end_bar"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_13"
                            android:layout_marginTop="@dimen/dp_12"
                            android:layout_marginRight="@dimen/dp_25"
                            android:max="300" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/end_bar_left_tv1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_14"
                                android:layout_marginTop="@dimen/dp_8"
                                android:text="0"
                                android:textColor="@color/c_101010"
                                android:textSize="@dimen/sp_12" />

                            <TextView
                                android:id="@+id/endt_bar_right_tv2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginTop="@dimen/dp_8"
                                android:layout_marginRight="@dimen/dp_20"
                                android:text="300"
                                android:textColor="@color/c_101010"
                                android:textSize="@dimen/sp_12" />
                        </RelativeLayout>
                        <!--混合类型-->
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_14"
                            android:layout_marginTop="@dimen/dp_11"
                            android:text="混合类型"
                            android:textColor="@color/c_000000"
                            android:textSize="@dimen/sp_12" />

                        <!--空气  高氧  氦氧氮混合-->
                        <!--<LinearLayout-->
                        <!--android:layout_width="match_parent"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:orientation="horizontal"-->
                        <!--android:layout_marginTop="@dimen/dp_4"-->
                        <!--&gt;-->
                        <android.support.design.widget.TabLayout
                            android:id="@+id/tab_add_o2"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_35"
                            android:layout_marginLeft="@dimen/dp_60"
                            android:layout_marginTop="@dimen/dp_4"
                            android:layout_marginRight="@dimen/dp_57"
                            android:background="@drawable/dive_log_selector_tab"
                            app:tabIndicatorHeight="@dimen/dp_0"
                            app:tabTextColor="@color/c_000000"
                            app:tabSelectedTextColor="@color/c_ffffff"
                            app:tabBackground="@drawable/dive_log_selector_tab"
                            />
                        <!--</LinearLayout>-->
                        <TextView
                            android:id="@+id/type_add3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_61"
                            android:layout_marginTop="@dimen/dp_16"
                            android:layout_marginRight="@dimen/dp_61"
                            android:layout_marginBottom="@dimen/dp_12"
                            android:background="@drawable/shape_et"
                            android:gravity="center"
                            android:text="+" />
                    </LinearLayout>

                    <!--配重-->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/sp_17"
                        android:text="@string/weight"
                        android:textColor="@color/blue"
                        android:textSize="@dimen/sp_14" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dp_31"
                        android:background="@drawable/shape_et"
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/dp_6"
                        tools:ignore="RtlHardcoded">

                        <EditText
                            android:id="@+id/weight"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:background="@null"
                            android:gravity="center"
                            android:hint="2"
                            android:inputType="phone|number"
                            android:lines="1"
                            android:maxLines="1"
                            android:textSize="@dimen/sp_14"
                            android:textStyle="normal"
                            tools:ignore="Autofill,HardcodedText" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
